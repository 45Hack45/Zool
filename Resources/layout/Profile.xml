<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:minWidth="25px"
    android:minHeight="25px"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/relativeLayout1">

    <android.support.v7.widget.Toolbar
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/toolbar" />

    <RelativeLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/relativeLayout2">
        <ImageView
            android:src="@drawable/arrow_left"
            android:layout_width="39.0dp"
            android:layout_height="34.0dp"
            android:id="@+id/BackArrow_image"
            android:layout_marginTop="7.0dp"
            android:layout_marginLeft="8.0dp" />
        <ImageView
            android:src="@drawable/account_circle"
            android:layout_width="wrap_content"
            android:layout_height="95.5dp"
            android:layout_toRightOf="@id/imageView1"
            android:id="@+id/IconaUsuari_image"
            android:layout_centerHorizontal="false"
            android:layout_centerVertical="false"
            android:layout_centerInParent="false"
            android:layout_alignParentLeft="false"
            android:layout_alignParentRight="true"
            android:layout_below="@id/imageView1"
            android:layout_alignParentTop="true"
            android:layout_marginRight="45.0dp"
            android:layout_marginTop="45.0dp"
            android:layout_marginLeft="0.0dp"/>
        <TextView
            android:text="NOM USUARI"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/imageView2"
            android:id="@+id/NomUsuari_text"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="0.0dp" />
        <ProgressBar
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="220.0dp"
            android:layout_height="wrap_content"
            android:id="@+id/progressBar1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="28.5dp"
            android:background="#333debd0"
            android:progressBackgroundTint="#333debd0"
            android:progressTint="#b23debd0" />
        <TextView
            android:text="Nivell actual"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="112.5dp"
            android:layout_height="32.0dp"
            android:layout_toLeftOf="@id/progressBar1"
            android:id="@+id/NivellActual_text"
            android:layout_centerVertical="false"
            android:layout_marginTop="197.0dp"
            android:layout_alignParentLeft="false"
            android:layout_alignParentRight="false" />
        <TextView
            android:text="Pròxim nivell"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/progressBar1"
            android:id="@+id/ProximNivell_text"
            android:layout_alignParentRight="true"
            android:layout_marginTop="196.5dp" />
        <TextView
            android:text="following"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/Following_text"
            android:layout_alignParentRight="true"
            android:layout_marginTop="16.0dp"
            android:layout_marginRight="90.0dp" />
        <TextView
            android:text="followers"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@id/relativeLayout2"
            android:id="@+id/Followers_text"
            android:layout_width="wrap_content"
            android:layout_centerVertical="false"
            android:layout_marginTop="232.0dp"
            android:layout_marginLeft="114.0dp" />
        <TextView
            android:text="nº"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text1"
            android:layout_marginTop="0.0dp"
            android:layout_marginLeft="94.0dp"
            android:textStyle="bold" />
        <TextView
            android:text="nº"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text2"
            android:layout_marginTop="0.0dp"
            android:layout_marginLeft="0.0dp"
            android:textStyle="bold"
            android:layout_alignParentLeft="false"
            android:layout_alignParentRight="false"
            android:layout_marginRight="6.0dp" />
        <TextView
            android:text="UPLOADED FIELDS"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/UPLOADEDFIELDS_text"
            android:layout_marginTop="24.0dp"
            android:layout_marginLeft="15.0dp"
            android:textStyle="bold" />
