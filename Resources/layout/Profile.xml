            android:text="FIELDS OF INTEREST"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/FIELDSOFINTEREST_text"
