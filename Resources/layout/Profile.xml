            android:layout_marginLeft="0.0dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="7.0dp"
            android:textStyle="bold" />
        <TextView
            android:text="SCIENCE"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ScienceUP_text"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@id/imageView3"
            android:layout_alignParentRight="false"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="217.0dp"
            android:layout_marginLeft="19.5dp"
            android:layout_marginTop="14.0dp"
            android:textStyle="bold"
            android:textColor="#ff585858" />
        <ImageView
            android:src="#403db5eb"
            android:layout_width="104.5dp"
            android:layout_height="27.5dp"
            android:id="@+id/imageView3"
            android:layout_marginLeft="15.0dp"
            android:layout_marginTop="9.0dp" />
        <ImageView
            android:src="@drawable/atom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/IconaScience1_image"
            android:layout_marginLeft="94.5dp"
            android:layout_marginTop="11.5dp"
            android:tint="#ff6b6b6b" />
        <ImageView
            android:src="#403db5eb"
            android:layout_width="108.5dp"
            android:layout_height="26.5dp"
            android:id="@+id/imageView5"
            android:layout_alignParentRight="true"
            android:layout_marginTop="9.5dp"
            android:layout_marginRight="70.5dp" />
        <ImageView
            android:src="@drawable/atom"
            android:layout_width="50.5dp"
            android:layout_height="25.5dp"
            android:id="@+id/IconaScience2_image"
            android:layout_alignParentRight="true"
            android:layout_marginRight="60.5dp"
            android:layout_marginTop="9.0dp"
            android:tint="#ff6b6b6b" />
        <TextView
            android:text="SCIENCE"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/ScienceFOI_text"
            android:layout_marginLeft="212.0dp"
            android:layout_marginTop="14.0dp"
            android:textStyle="bold" />
        <ImageView
            android:src="@android:drawable/ic_menu_gallery"
            android:layout_width="384.5dp"
            android:layout_height="wrap_content"
            android:id="@+id/imageView7"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@id/imageView1"
            android:layout_alignParentTop="false"
            android:layout_centerInParent="false"
            android:layout_centerVertical="false"
            android:layout_marginTop="271.0dp"
            android:layout_marginLeft="0.0dp"
            android:layout_alignParentLeft="true" />
    </RelativeLayout>
</RelativeLayout>