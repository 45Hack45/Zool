<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:minWidth="25px"
    android:minHeight="25px"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/relativeLayout1">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="244.0dp"
        android:id="@+id/imageView1"
        android:layout_centerInParent="true"
        android:layout_alignParentTop="true"
        android:fadingEdge="none"
        android:background="#403db5eb"
        android:fitsSystemWindows="true"
        android:requiresFadingEdge="none"
        android:layout_marginBottom="0.0dp" />
    <ImageView
        android:src="@drawable/account_circle"
        android:layout_width="53.0dp"
        android:layout_height="49.5dp"
        android:layout_below="@id/imageView1"
        android:id="@+id/imageView2"
        android:layout_alignParentStart="false"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="5.5dp"
        android:layout_marginTop="7.0dp" />
    <TextView
        android:text="NOM USUARI"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_width="135.5dp"
        android:layout_height="42.0dp"
        android:layout_toRightOf="@id/imageView2"
        android:id="@+id/textView1"
        android:layout_alignParentEnd="false"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="false"
        android:layout_centerHorizontal="true"
        android:fadeScrollbars="false"
        android:scrollbarAlwaysDrawHorizontalTrack="false"
        android:scrollbarAlwaysDrawVerticalTrack="false"
        android:layout_marginLeft="3.0dp"
        android:scrollbars="none"
        android:foregroundGravity="bottom"
        android:foregroundInsidePadding="false"
        android:textStyle="normal"
        android:fadingEdge="none"
        android:allowUndo="true"
        android:gravity="bottom"
        android:layout_marginBottom="1.0dp"
        android:layout_alignParentBottom="false"
        android:layout_marginTop="3.0dp"
        android:layout_marginRight="0.0dp" />
    <TextView
        android:text="SCIENCE"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="101.5dp"
        android:layout_height="36.5dp"
        android:layout_below="@id/textView1"
        android:id="@+id/textView2"
        android:gravity="right|bottom"
        android:textStyle="bold"
        android:layout_alignParentEnd="false"
        android:layout_marginTop="17.0dp" />
    <ImageView
        android:src="@drawable/atom"
        android:layout_width="wrap_content"
        android:layout_height="21.5dp"
        android:layout_toRightOf="@id/textView2"
        android:id="@+id/imageView3"
        android:layout_below="@id/textView1"
        android:importantForAutofill="no"
        android:layout_alignParentRight="false"
        android:foregroundGravity="right"
        android:layout_alignParentEnd="true"
        android:layout_toLeftOf="@id/imageView3"
        android:layout_marginLeft="14.0dp"
        android:layout_marginRight="189.5dp"
        android:layout_marginTop="30.5dp"
        android:foregroundInsidePadding="false"
        android:foregroundTint="#ff212121" />
    <ImageView
        android:src="#1a3db5eb"
        android:layout_width="194.0dp"
        android:layout_height="53.0dp"
        android:layout_below="@id/textView2"
        android:id="@+id/imageView4"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="0.0dp"
        android:layout_marginLeft="5.0dp"
        android:layout_marginTop="6.0dp"
        android:layout_marginRight="0.0dp" />
    <TextView
        android:text="PHYSICS"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="77.5dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView2"
        android:id="@+id/textView3"
        android:layout_marginLeft="28.5dp"
        android:layout_marginTop="21.0dp"
        android:layout_marginRight="0.0dp" />
    <TextView
        android:text="GRAVITATION"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView2"
        android:id="@+id/textView4"
        android:layout_marginTop="68.0dp"
        android:layout_marginLeft="29.0dp" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="18.5dp"
        android:layout_toRightOf="@id/textView4"
        android:id="@+id/imageView5"
        android:layout_alignParentLeft="false"
        android:layout_alignParentRight="true"
        android:layout_marginTop="123.5dp"
        android:layout_marginRight="191.5dp"
        android:src="@drawable/radioactive" />
    <ImageButton
        android:src="@android:drawable/ic_menu_gallery"
        android:layout_width="189.0dp"
        android:layout_height="161.5dp"
        android:layout_below="@id/imageView1"
        android:id="@+id/imageButton1"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="54.5dp"
        android:layout_marginLeft="1.0dp" />
    <ImageView
        android:src="@drawable/star_outline"
        android:layout_width="24.0dp"
        android:layout_height="24.0dp"
        android:layout_below="@id/imageButton1"
        android:id="@+id/imageView6"
        android:layout_centerVertical="false"
        android:layout_alignParentRight="true"
        android:layout_marginRight="6.5dp"
        android:layout_marginTop="0.0dp" />
    <EditText
        android:inputType="number"
        android:layout_width="24.0dp"
        android:layout_height="25.5dp"
        android:layout_below="@id/imageView6"
        android:id="@+id/editText1"
        android:layout_marginTop="212.0dp"
        android:layout_marginLeft="0.0dp"
        android:layout_marginBottom="4.0dp"
        android:layout_centerInParent="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="29.5dp" />
    <TextView
        android:text="Comments"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/imageButton1"
        android:id="@+id/textView5"
        android:layout_alignParentRight="true"
        android:layout_marginTop="1.0dp"
        android:layout_marginRight="91.5dp" />
    <ImageView
        android:src="@drawable/comment_text_multiple_outline"
        android:layout_width="18.0dp"
        android:layout_height="22.5dp"
        android:layout_below="@id/textView5"
        android:id="@+id/imageView7"
        android:layout_alignParentRight="true"
        android:layout_marginTop="215.0dp"
        android:layout_marginRight="165.5dp"
        android:layout_alignParentBottom="false"
        android:layout_alignParentTop="true" />
    <ImageView
        android:src="@drawable/notebook_outline"
        android:layout_width="40.0dp"
        android:layout_height="42.5dp"
        android:layout_toRightOf="@id/imageView4"
        android:id="@+id/imageView8"
        android:layout_marginLeft="155.0dp"
        android:layout_marginTop="10.0dp"
        android:layout_marginBottom="8.0dp"
        android:layout_marginRight="3.0dp" />
</RelativeLayout>